{
  "manifest_version": 2,
  "name": "Zotshort",
  "version": "0.003-alpha",
  "description": "A Zotero plugin that adds convenient keyboard shortcuts for commonly used functions.",
  "homepage_url": "https://github.com/alon/zotshort",
  "author": "Alon",
  "icons": {
    "48": "content/icons/favicon@0.5x.png",
    "96": "content/icons/favicon.png"
  },
  "applications": {
    "zotero": {
      "id": "zotshort@alon.com",
      "update_url": "https://github.com/alon/zotshort/releases",
      "strict_min_version": "6.999",
      "strict_max_version": "7.*"
    }
  }
}
